    "a = int(input('enter a number '))\n",
    "n = 0\n",
    "b = 0\n",
    "while n <= a :\n",
    "    n +=1\n",
    "    if a%n == 0:\n",
    "        b +=1\n",
    "if b ==2 :\n",
    "    print(a,' is a prime number')\n",
    "elif b ==1:\n",
    "    print(a,' is neither composite nor prime')\n",
    "else :\n",
    "    print(a,' is a composite number')\n",
